# Podfile
platform :ios, '10.0'

target 'RXSwift_learning_DayByDay' do
  use_frameworks!

  pod 'RxSwift'
  pod 'RxCocoa'

  #SwiftLint 是 realm 公司开发的一个插件，专门用于管理 Swift 代码的规范
  pod 'SwiftLint'
  #消息提示
  pod 'SwiftMessages'

  end

# RxTests 和 RxBlocking 将在单元/集成测试中起到重要作用
target 'RXSwift_learning_DayByDayTests' do
  pod 'RxBlocking', '~> 4.0'
  pod 'RxTest',     '~> 4.0'
end

